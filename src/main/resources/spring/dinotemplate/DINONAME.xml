<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="juvenile/DINONAMEvaluesjuvi.xml"/>
    <import resource="adult/DINONAMEvaluesadult.xml"/>

    <bean id="DINONAME" class="rottingalien.theisledmgcalc.dinoinfo.Dino">

        <property name="name" value="DINONAME"/>
        <property name="growthStateMap">
            <map>
                <entry key="freshJuvi" value-ref="DINONAMEFreshJuvi"/>
                <entry key="fullJuvi" value-ref="DINONAMEFullJuvi"/>
                <entry key="freshSub" value-ref="DINONAMEFreshSub"/>
                <entry key="fullSub" value-ref="DINONAMEFullSub"/>
                <entry key="freshAdult" value-ref="DINONAMEFreshAdult"/>
                <entry key="fullAdult" value-ref="DINONAMEFullAdult"/>
            </map>
        </property>
    </bean>

    <!-- JUVENILE !-->

    <bean id="DINONAMEFreshJuvi" class="rottingalien.theisledmgcalc.dinoinfo.GrowthState">
        <property name="name" value="Fresh Juvenile"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value="DINONAMEFreshJuviWeight"/>
                <entry key="health" value="DINONAMEFreshJuviHealth"/>
                <entry key="biteforce" value="DINONAMEFreshJuviBiteForce"/>
                <entry key="bleed" value="DINONAMEFreshJuviBleed"/>
                <entry key="bleedStandResist" value="DINONAMEFreshJuviBleedStandResist"/>
                <entry key="bleedSitResist" value="DINONAMEFreshJuviBleedSitResist"/>
                <entry key="bleedWalkResist" value="DINONAMEFreshJuviBleedWalkResist"/>
                <entry key="bleedRunResist" value="DINONAMEFreshJuviBleedRunResist"/>

            </map>
        </property>
    </bean>

    <bean id="DINONAMEFullJuvi" class="rottingalien.theisledmgcalc.dinoinfo.GrowthState">
        <property name="name" value="Full Juvenile"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value="DINONAMEFullJuviWeight"/>
                <entry key="health" value="DINONAMEFullJuviHealth"/>
                <entry key="biteforce" value="DINONAMEFullJuviBiteForce"/>
                <entry key="bleed" value="DINONAMEFullJuviBleed"/>
                <entry key="bleedStandResist" value="DINONAMEFullJuviBleedStandResist"/>
                <entry key="bleedSitResist" value="DINONAMEFullJuviBleedSitResist"/>
                <entry key="bleedWalkResist" value="DINONAMEFullJuviBleedWalkResist"/>
                <entry key="bleedRunResist" value="DINONAMEFullJuviBleedRunResist"/>
            </map>
        </property>
    </bean>

    <!-- SUB-ADULT !-->

    <bean id="DINONAMEFreshSub" class="rottingalien.theisledmgcalc.dinoinfo.GrowthState">
        <property name="name" value="Fresh Sub-Adult"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value="DINONAMEFreshSubWeight"/>
                <entry key="health" value="DINONAMEFreshSubHealth"/>
                <entry key="biteforce" value="DINONAMEFreshSubBiteForce"/>
                <entry key="bleed" value="DINONAMEFreshSubBleed"/>
                <entry key="bleedStandResist" value="DINONAMEFreshSubBleedStandResist"/>
                <entry key="bleedSitResist" value="DINONAMEFreshSubBleedSitResist"/>
                <entry key="bleedWalkResist" value="DINONAMEFreshSubBleedWalkResist"/>
                <entry key="bleedRunResist" value="DINONAMEFreshSubBleedRunResist"/>

            </map>
        </property>
    </bean>

    <bean id="DINONAMEFullSub" class="rottingalien.theisledmgcalc.dinoinfo.GrowthState">
        <property name="name" value="Full Sub-Adult"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value="DINONAMEFullSubWeight"/>
                <entry key="health" value="DINONAMEFullSubHealth"/>
                <entry key="biteforce" value="DINONAMEFullSubBiteForce"/>
                <entry key="bleed" value="DINONAMEFullSubBleed"/>
                <entry key="bleedStandResist" value="DINONAMEFullSubBleedStandResist"/>
                <entry key="bleedSitResist" value="DINONAMEFullSubBleedSitResist"/>
                <entry key="bleedWalkResist" value="DINONAMEFullSubBleedWalkResist"/>
                <entry key="bleedRunResist" value="DINONAMEFullSubBleedRunResist"/>
            </map>
        </property>
    </bean>

    <!-- ADULT !-->

    <bean id="DINONAMEFreshAdult" class="rottingalien.theisledmgcalc.dinoinfo.GrowthState">
        <property name="name" value="Fresh Adult"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value="DINONAMEFreshAdultWeight"/>
                <entry key="health" value="DINONAMEFreshAdultHealth"/>
                <entry key="biteforce" value="DINONAMEFreshAdultBiteForce"/>
                <entry key="bleed" value="DINONAMEFreshAdultBleed"/>
                <entry key="bleedStandResist" value="DINONAMEFreshAdultBleedStandResist"/>
                <entry key="bleedSitResist" value="DINONAMEFreshAdultBleedSitResist"/>
                <entry key="bleedWalkResist" value="DINONAMEFreshAdultBleedWalkResist"/>
                <entry key="bleedRunResist" value="DINONAMEFreshAdultBleedRunResist"/>
            </map>
        </property>
    </bean>

    <bean id="DINONAMEFullAdult" class="rottingalien.theisledmgcalc.dinoinfo.GrowthState">
        <property name="name" value="Full Adult"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value="DINONAMEFullAdultWeight"/>
                <entry key="health" value="DINONAMEFullAdultHealth"/>
                <entry key="biteforce" value="DINONAMEFullAdultBiteForce"/>
                <entry key="bleed" value="DINONAMEFullAdultBleed"/>
                <entry key="bleedStandResist" value="DINONAMEFullAdultBleedStandResist"/>
                <entry key="bleedSitResist" value="DINONAMEFullAdultBleedSitResist"/>
                <entry key="bleedWalkResist" value="DINONAMEFullAdultBleedWalkResist"/>
                <entry key="bleedRunResist" value="DINONAMEFullAdultBleedRunResist"/>
            </map>
        </property>
    </bean>
</beans>
