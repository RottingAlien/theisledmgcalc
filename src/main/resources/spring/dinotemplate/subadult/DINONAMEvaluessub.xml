<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- SUB-ADULT !-->

    <bean id="DINONAMEFreshSubWeight" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Weight"/>
        <property name="value" value="150"/>
    </bean>

    <bean id="DINONAMEFreshSubHealth" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Health"/>
        <property name="value" value="120"/>
    </bean>

    <bean id="DINONAMEFreshSubBiteForce" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Bite Force"/>
        <property name="value" value="50"/>
    </bean>

    <bean id="DINONAMEFreshSubBleed" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Bleed"/>
        <property name="value" value="15"/>
    </bean>

    <bean id="DINONAMEFreshSubBleedStandResist" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Standing Bleed Resistance"/>
        <property name="value" value="30"/>
    </bean>

    <bean id="DINONAMEFreshSubBleedSitResist" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Sitting Bleed Resistance"/>
        <property name="value" value="10"/>
    </bean>

    <bean id="DINONAMEFreshSubBleedWalkResist" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Walking Bleed Resistance"/>
        <property name="value" value="50"/>
    </bean>

    <bean id="DINONAMEFreshSubBleedRunResist" class="rottingalien.theisledmgcalc.dinoinfo.Property">
        <property name="name" value="Running Bleed resistance"/>
        <property name="value" value="100"/>
    </bean>


</beans>