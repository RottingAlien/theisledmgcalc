<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">



    <bean id="DINONAME" class="rottingalien.theisledmgcalc.dinoinfo.Dino">

        <property name="name" value="DINONAME"/>
        <property name="growthStateMap">
            <map>
                <entry key="freshJuvi" value-ref="DINONAMEFreshJuvi"/>
                <entry key="fullJuvi" value-ref="DINONAMEFullJuvi"/>
                <entry key="freshSub" value-ref="DINONAMEFreshSub"/>
                <entry key="fullSub" value-ref="DINONAMEFullSub"/>
                <entry key="freshAdult" value-ref="DINONAMEFreshAdult"/>
                <entry key="fullAdult" value-ref="DINONAMEFullAdult"/>
            </map>
        </property>
    </bean>

    <!-- JUVENILE !-->

