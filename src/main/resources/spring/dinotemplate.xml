                <entry key="biteforce" value=""/>
                <entry key="bleed" value=""/>
                <entry key="bleedStandResist" value=""/>
                <entry key="bleedSitResist" value=""/>
                <entry key="bleedWalkResist" value=""/>
                <entry key="bleedRunResist" value=""/>
            </map>
        </property>
    </bean>

    <!-- ADULT !-->

    <bean id="freshAdult" class="rottingalien.theisledmgcalc.GrowthState">
        <property name="name" value="Fresh Adult"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value=""/>
                <entry key="health" value=""/>
                <entry key="biteforce" value=""/>
                <entry key="bleed" value=""/>
                <entry key="bleedStandResist" value=""/>
