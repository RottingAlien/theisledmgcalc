        </property>
    </bean>

    <bean id="fullSub" class="rottingalien.theisledmgcalc.GrowthState">
        <property name="name" value="Full Sub-Adult"/>
        <property name="dinoPropertyMap">
            <map>
                <entry key="weight" value=""/>
